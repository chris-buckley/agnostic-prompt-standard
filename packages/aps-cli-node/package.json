{
  "name": "@agnostic-prompt/aps",
  "version": "1.1.5",
  "description": "CLI to install and manage the Agnostic Prompt Standard (APS) skill and platform templates.",
  "type": "module",
  "bin": {
    "aps": "./bin/aps.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:test": "tsc -p tsconfig.test.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run typecheck",
    "test": "npm run build && npm run build:test && node --test ./dist-test/*.test.js",
    "prepack": "npm run build && node ./scripts/prepack.js",
    "prepare": "npm run prepack"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "bin/",
    "dist/",
    "payload/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chris-buckley/agnostic-prompt-standard.git"
  },
  "license": "MIT"
}
